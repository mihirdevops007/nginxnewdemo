{
    "family": t2.medium,
    "containerDefinitions": [
        {
            "image": "543050024229.dkr.ecr.ap-northeast-1.amazonaws.com/nginxnew:5",
            "name": NginxNew,
	    "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
             "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn": arn:aws:iam::aws:policy/service-role/AmazonECSTaskExecutionRolePolicy
}
