{
    "family": t2.medium,
    "containerDefinitions": [
        {
            "image": ${AWS_ACCOUNT_ID}.dkr.ecr.${AWS_DEFAULT_REGION}.amazonaws.com/${${IMAGE_TAG}},
            "name": NginxNew,
	    "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 80
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
             "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn": arn:aws:iam::aws:policy/service-role/AmazonECSTaskExecutionRolePolicy
}
