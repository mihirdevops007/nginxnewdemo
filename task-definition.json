{
    "family": "nginx-app",
    "containerDefinitions": [
        {
            "image": "514523777807.dkr.ecr.us-east-1.amazonaws.com/nginxdemo",
            "name": "nginx-app",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn":  "arn:aws:iam::514523777807:role/JeenkinsCICD_EC2"
}
