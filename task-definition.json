{
  "family": "nginx-sample",
  "containerDefinitions": [
    {
      "name": "nginxrepo",
      "image": "514523777807.dkr.ecr.us-east-1.amazonaws.com/nginxdemo:IMAGE_TAG_PLACEHOLDER",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 3000,
          "hostPort": 80
        }
      ]
    }
  ],
  "cpu": "256",
  "memory": "512",
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::514523777807:role/JeenkinsCICD_EC2"
}
