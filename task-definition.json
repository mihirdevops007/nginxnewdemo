{
    "family": "Web-app",
    "containerDefinitions": [
        {
            "image": "543050024229.dkr.ecr.ap-northeast-1.amazonaws.com/nodejs",
            "name": "Web-app",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn":  "arn:aws:iam::543050024229:role/ecsTaskExecutionRole"
}
