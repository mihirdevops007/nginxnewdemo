{
    "family": "nginx-app",
    "containerDefinitions": [
        {
            "image": "514523777807.dkr.ecr.us-east-1.amazonaws.com/nginxdemo",
            "name": "nginx-app",
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn": "arn:aws:iam::514523777807:role/JeenkinsCICD_EC2",
    "loadBalancers": [
        {
            "targetGroupArn": "arn:aws:elasticloadbalancing:us-east-1:514523777807:targetgroup/nginxTG/0e3cb261e7801edd",
            "containerName": "nginx-app",
            "containerPort": 80
        }
    ]
}

