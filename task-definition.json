{
  "family": "nginx-dev",
  "containerDefinitions": [
    {
      "name": "nginxrepo",
      "image": "514523777807.dkr.ecr.us-east-1.amazonaws.com/nginxdemo",
      "essential": true,
      "portMappings": [
        {
          "containerPort": 80,
          "hostPort": 80
        }
      ]
    }
  ],
  "cpu": "256",     // Updated CPU value (in CPU units)
  "memory": "512",  // Updated memory value (in MiB)
  "networkMode": "awsvpc",
  "executionRoleArn": "arn:aws:iam::514523777807:role/JeenkinsCICD_EC2"
}
