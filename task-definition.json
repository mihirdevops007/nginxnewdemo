{
    "family": $FAMLI,
    "containerDefinitions": [
        {
            "image": $REPOSITORY_URI,
            "name": $NAME,
	    "essential": true,
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 80
                }
            ]
        }
    ],
    "cpu": "256",
    "memory": "512",
    "requiresCompatibilities": [
             "FARGATE"
    ],
    "networkMode": "awsvpc",
    "executionRoleArn":  $ROLE_ARN
}
